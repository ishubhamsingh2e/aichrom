{
  "version": 2,
  "builds": [
    {
      "src": "aichrom/wsgi.py",
      "use": "@vercel/python",
      "config": {
          "runtime": "python3.9"
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "aichrom/wsgi.py"
    }
  ]
}